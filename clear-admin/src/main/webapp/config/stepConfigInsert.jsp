->						<!-- BEGIN PAGE TITLE & BREADCRUMB-->						<h3 class="page-title">							清算流程配置 <small>Liquidation Step Config</small>						</h3>						<ul class="breadcrumb">							<li><i class="icon-home"></i> <a href="<%=request.getContextPath()%>/admin/admin.jsp">首页</a> <i								class="icon-angle-right"></i></li>							<li><a href="<%=request.getContextPath()%>/config/stepConfig.jsp">清算流程管理</a> <i class="icon-angle-right"></i></li>							<li><a href="<%=request.getContextPath()%>/config/stepConfig.jsp">清算流程配置</a></li>						</ul>						<!-- END PAGE TITLE & BREADCRUMB-->					</div>				</div>				<!-- END PAGE HEADER-->				<!-- BEGIN PAGE CONTENT-->					<div class="row-fluid">					<div class="span12">						<!-- BEGIN SAMPLE FORM PORTLET-->   						<div class="portlet box blue tabbable">							<div class="portlet-title">								<div class="caption">									<i class="icon-reorder"></i>									<span class="hidden-480">增加清算场次</span>								</div>							</div>							<div class="portlet-body form">									<div class="tab-content">										<div class="tab-pane active" id="portlet_tab1">										<br>											<!-- BEGIN FORM-->											<form:form method="post" modelAttribute="clearStepDO" cssClass="form-horizontal">												<div class="control-group">													<label class="control-label">场次号</label>													<div class="controls">														<form:input path="step" cssClass="m-wrap small" name="stepSeq" required="true" pattern="^[1-9]\d*$" placeholder="场次号，整数，必填"/>																												<form:errors path="step" cssClass="label label-important"/>													</div>												</div>												<div class="control-group">													<label class="control-label">场次名称</label>													<div class="controls">														<form:input path="name" cssClass="m-wrap medium" name="stepName" required="true" placeholder="场次名称，必须填写"/>																												<form:errors path="name" cssClass="label label-important"/>													</div>												</div>																								<div class="control-group">													<label class="control-label">应用IP</label>													<div class="controls">														<form:input path="ip" cssClass="m-wrap large" name="stepIp" />																												<span class="help-inline">场次应用所在IP，默认本机</span>													</div>												</div>												<div class="control-group">													<label class="control-label">应用路径</label>													<div class="controls">														<form:input path="url" cssClass="m-wrap large" name="stepUrl" required="true" placeholder="应用路径，必须填写"/>																												<form:errors path="url" cssClass="label label-important"/>														<span class="help-inline">场次执行程序路径,其中&#36;&#123;CUR_DATE&#125;字段会被替换成清算日期</span>													</div>																								</div>																								<div class="control-group">													<label class="control-label">日志路径</label>													<div class="controls">														<form:input path="logfile" cssClass="m-wrap large" name="stepLogfile"/>														<span class="help-inline">场次明细日志路径,其中&#36;&#123;CUR_DATE&#125;字段会被替换成清算日期</span>													</div>																								</div>																								<div class="control-group">													<label class="control-label">备注</label>													<div class="controls">														<form:input path="comt" cssClass="m-wrap large" name="stepComt"/>													</div>																								</div>												<div class="form-actions">													<button type="submit" class="btn blue"><i class="icon-ok"></i> 保存</button>													<a class="btn" href="<%=request.getContextPath()%>/config/stepConfig.jsp">取消</a>												</div>											</form:form>											<!-- END FORM-->  										</div>									</div>								</div>							</div>						</div>					</div>				<!-- END PAGE CONTENT-->			</div>		